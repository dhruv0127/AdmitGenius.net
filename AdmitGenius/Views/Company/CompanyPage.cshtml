            max-width: 1200px;
            margin: 0 auto;
        }

        .job-title {
            font-size: larger;
            margin-bottom: 10px;
            font-family: sans-serif;
            margin-top: 2px;
        }

        .posted-time {
            display: flex;
            align-items: center;
            color: #a5a5a5;
            font-size: 15px;
            margin-top: 5px;
