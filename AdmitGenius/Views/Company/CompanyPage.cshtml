@model AdmitGenius.Models.Company

<style>
    #home {
        background-image: linear-gradient(to bottom, rgba(139,186,244, 0.52), rgba(15,51,92, 0.73) ), url('https://assets.architecturaldigest.in/photos/6008499aa8f8397741704a19/16:9/w_1920,c_limit/Ronnie-Screwvala-mumbai-RSVP-office_2-2-1366x768.jpg');
        width: 100%;
        height: 108vh;
        background-size: cover;
        color: white;
        display: flex;
        justify-content: center;
        align-items: center;
    }

    /* 14 */
    .btn-14 {
        background-image: linear-gradient(to right, #1FA2FF 0%, #12D8FA 51%, #1FA2FF 100%);
        border: none;
        padding: 10px;
        border-radius: 7px;
        z-index: 1;
        text-transform: uppercase;
    }

        .btn-14:after {
            position: absolute;
            content: "";
            width: 100%;
            height: 0;
            top: 0;
            left: 0;
            z-index: -1;
            border-radius: 5px;
            background-color: #74EBD5;
            background-image: linear-gradient(90deg, #74EBD5 0%, #9FACE6 100%);
            box-shadow: inset 2px 2px 2px 0px rgba(255,255,255,.5);
            7px 7px 20px 0px rgba(0,0,0,.1), 4px 4px 5px 0px rgba(0,0,0,.1);
            transition: all 0.3s ease;
        }

        .btn-14:hover {
            color: #000;
        }

            .btn-14:hover:after {
                top: auto;
                bottom: 0;
                height: 100%;
            }

        .btn-14:active {
            top: 2px;
        }

    .button4 {
        margin: 10px;
        padding: 7px;
        text-align: center;
        text-transform: uppercase;
        transition: 0.5s;
        background-size: 200% auto;
        color: white;
        border-radius: 10px;
        display: block;
        position: relative;
        overflow: hidden;
    }

        .button4:hover {
            background-position: right center;
            color: #fff;
            text-decoration: none;
            box-shadow: 0 0 0 0 rgba(255, 255, 255, 0.7);
            animation: spread-border 1s infinite;
        }

    @@keyframes spread-border {
        to {
            box-shadow: 0 0 0 10px rgba(255, 255, 255, 0);
        }
    }

</style>



