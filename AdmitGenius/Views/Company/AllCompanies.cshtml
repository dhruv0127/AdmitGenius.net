    <script>
        const textElement = document.getElementById('text');
        const jobText = "Job";
        const internshipText = "Internship";
        let isJob = true;

        function animateText(index) {
            const currentText = textElement.innerText;
            const nextText = isJob ? internshipText.substring(0, index + 1) : jobText.substring(0, index + 1);
            textElement.innerText = nextText;

            if (nextText !== (isJob ? internshipText : jobText)) {
                setTimeout(() => animateText(index + 1), 100); // Adjust the delay as needed
            } else {
                isJob = !isJob; // Switch between Job and Internship
                setTimeout(() => animateText(0), 1000); // Delay before switching
            }
        }

        // Start the animation
        animateText(0);
    </script>

