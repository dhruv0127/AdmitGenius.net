    <script>
        $(document).ready(function () {
            // Loop to generate event handlers for Back and Forward buttons
            for (var i = 0; i <= 15; i++) {
                $("#back" + i).on("click", createScrollHandler(i, -65, -1));
                $("#fwd" + i).on("click", createScrollHandler(i, -65, 1));
            }

            // Function to generate scroll event handlers
            function createScrollHandler(index, customHeight, direction) {
                return function () {
                    // Calculate target element index based on direction
                    var targetIndex = index + direction;

                    // Scroll to the center of the target element with custom offset
                    $("html, body").animate({
                        scrollTop: $(".q" + targetIndex).offset().top - ($(window).height() - $(".q" + targetIndex).outerHeight()) / 2 + customHeight
                    }, 1000);
                };
            }
        });
    </script>

